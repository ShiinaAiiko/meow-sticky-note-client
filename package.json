{
	"name": "meow-sticky-note",
	"version": "1.0.1",
	"private": true,
	"description": "一个简单易用的跨平台笔记",
	"main": "./src/electron/dist/main.js",
	"author": "Shiina Aiiko <shiina@aiiko.club>",
	"homepage": ".",
	"scripts": {
		"cache": "cp ./public/package1.json ./node_modules/@saki-ui/core/package.json",
		"dc": "sudo rm -rf ./node_modules/.cache",
		"dev": "cp -r ./config.test.json ./src/config.temp.json && yarn dc && PORT=16111 react-scripts start  --allow-root",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"start": "concurrently \"BROWSER=none yarn dev\" \"wait-on http://localhost:3000 && electron .\"",
		"el:dev": "nodemon --exec electron ./src/electron/dist/main.js",
		"el:tsc": "rm -rf ./src/electron/dist/ && cd ./src/electron && tsc",
		"el:build": "./release.sh elbuild",
		"el:icon": "electron-icon-builder --input=./public/logo-neko-circle-white1500.png --output=public --flatten",
		"protos:build": "pbjs -t static-module -w commonjs -o src/protos/proto.js  ./protos_temp/**/*.proto",
		"protos:tsc": "pbts -o ./src/protos/proto.d.ts ./src/protos/proto.js",
		"protos": "yarn protos:build && yarn protos:tsc"
	},
	"build": {
		"productName": "Meow Sticky Note",
		"appId": "com.meow.sticky.note",
		"copyright": "©2022 Shiina Aiiko",
		"directories": {
			"output": "el-build"
		},
		"files": [
			"src/electron/dist/**/*",
			"build/**/*",
			"package.json"
		],
		"extraResources": {
			"from": "./out/",
			"to": "out/"
		},
		"nsis": {
			"shortcutName": "ExcelUtil",
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"linux": {
			"icon": "build/icons",
			"target": [
				"deb"
			]
		},
		"extends": null
	},
	"watch": [
		"src/electron/dist/*"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@nyanyajs/utils": "^1.0.12",
		"@reduxjs/toolkit": "^1.8.3",
		"@saki-ui/core": "^1.0.0",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"blueimp-md5": "^2.19.0",
		"buffer": "^6.0.3",
		"electron-is-dev": "^2.0.0",
		"electron-reload": "^2.0.0-alpha.1",
		"protobufjs": "^6.11.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-helmet-async": "^1.3.0",
		"react-i18next": "^11.18.0",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.1",
		"react-transition-group": "^4.4.5",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@types/blueimp-md5": "^2.18.0",
		"@types/node": "^18.0.4",
		"@types/react-helmet": "^6.1.5",
		"@types/react-transition-group": "^4.4.5",
		"@types/socket.io-client": "^1.4.36",
		"@types/uuid": "^8.3.4",
		"concurrently": "^7.2.2",
		"electron": "^19.0.7",
		"electron-builder": "^23.1.0",
		"sass": "^1.53.0",
		"typescript": "^4.7.4",
		"wait-on": "^6.0.1"
	}
}
